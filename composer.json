{
    "name": "chrgriffin/laravel-collection-package-generator",
    "description": "Laravel application installer.",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Griffin",
            "email": "christian.griffin@live.com"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/console": "^4.0|^5.0",
        "symfony/filesystem": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/collection"
    ],
    "autoload": {
        "psr-4": {
            "LaravelCollectionPackageGenerator\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelCollectionPackageGenerator\\Console\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
